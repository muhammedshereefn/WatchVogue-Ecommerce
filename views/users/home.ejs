<style>
    .featured img {
        transition: transform 0.3s ease-in-out, box-shadow 0.3s ease-in-out, opacity 0.3s ease-in-out;
    }

    .featured img:hover {
        transform: translateY(-10px); /* Move the image 10 pixels upwards */
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.3); /* Adjust shadow properties as needed */
        opacity: 0.8; /* Adjust the opacity value */
    }
</style>
<style>
    .newsletter {
        position: relative;
        background-color: #333;
        color: white;
    }

    .video-container {
        width: 100%;
        height: 90px; /* Set the desired height */
        overflow: hidden;
    }

    video {
        width: 100%;
        height: 100%; /* Ensure the video fills the container */
        object-fit: cover; /* Maintain aspect ratio and cover the container */
    }

    .content-container {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
    }

    .subscribe-form {
        display: flex;
        align-items: center;
    }

    .watch-icon {
        width: 40px; /* Adjust the size as needed */
        height: 40px; /* Adjust the size as needed */
        margin-right: 10px;
        animation: rotateWatch 2s infinite linear;
    }

    @keyframes rotateWatch {
        0% {
            transform: rotate(0deg);
        }
        100% {
            transform: rotate(360deg);
        }
    }

    .form-control {
        width: 100%;
        padding: 10px;
        margin-bottom: 10px;
        border: 1px solid #fff;
        border-radius: 5px;
        background-color: transparent;
        color: #fff;
        transition: border-color 0.3s ease-in-out;
    }

    .form-control:focus {
        border-color: #ffcc00;
        outline: none;
    }

    .subscribe-btn {
        background-color: #ffcc00;
        color: #333;
        padding: 10px 20px;
        border: none;
        border-radius: 5px;
        cursor: pointer;
        transition: background-color 0.3s ease-in-out, transform 0.3s ease-in-out;
    }

    .subscribe-btn:hover {
        background-color: #ffd633;
        transform: scale(1.1);
    }
</style>

<style>
    .product-cart-wrap {
